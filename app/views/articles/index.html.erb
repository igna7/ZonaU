<div class="field">
	<% @articles.each do |article| %>
	<h1><%= link_to article.title, article %></h1>
		<div>
			<%= article.body %>
		</div>
	<%end%>
</div>
<div class="container-fluid">
	<div class="row">
			<div class="zonau_pagination space_pagination">
				<%= will_paginate @articles, previous_label: "Anterior", next_label: "Siguiente" , :page_links => false%>
			</div>
	</div>
</div>
